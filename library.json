{
    "name": "libdaisy",
    "version": "0.0.1",
    "build": {
        "extraScript": "./extra_build_script.py",
		"srcDir" : "./",
        "srcFilter": [
            "+<src/*>",
			"+<Drivers/*>",
			"+<Drivers/CMSIS/include/*>",
			"+<Drivers/CMSIS/DSP/*>",
            "+<Middlewares/*>",
            "+<core/*>"
        ],
        "flags": [
            "-std=c++11",
            "-DARM_MATH_CM7",
            "-DCORE_CM7",
            "-DSTM32H750xx",
            "-DSTM32H750IB",
            "-DARM_MATH_CM7",
            "-Dflash_layout",
            "-DHSE_VALUE=16000000",
            "-DUSE_HAL_DRIVER",
            "-DUSE_FULL_LL_DRIVER ",
            "-IDrivers/CMSIS/Include",
            "-IDrivers/CMSIS/Device/ST/STM32H7xx/Include",
            "-IDrivers/STM32H7xx_HAL_Driver/Inc",
            "-IDrivers/STM32H7xx_HAL_Driver/Inc/Legacy",
            "-IMiddlewares/ST/STM32_USB_Device_Library/Core/Inc",
            "-IMiddlewares/ST/STM32_USB_Device_Library/Class/CDC/Inc",
            "-IMiddlewares/Third_Party/FatFs/src",
            "-Isrc",
            "-Isrc/sys",
            "-Isrc/dev/codec_ak4556",
            "-Isrc/sys/dma",
            "-Isrc/hid/audio",
            "-Isrc/dev/sdram",
            "-Isrc/sys/fatfs",
            "-Isrc/sys/system",
            "-Isrc/per/dac",
            "-Isrc/per/gpio",
            "-Isrc/per/qspi",
            "-Isrc/per/sai",
            "-Isrc/per/sdmmc",
            "-Isrc/per/tim",
            "-Isrc/dev/sr_4021",
            "-Isrc/util/bsp_sd_diskio",
            "-Isrc/util/hal_map",
            "-Isrc/util/oled_fonts",
            "-Isrc/util/sd_diskio",
            "-Isrc/util/unique_id",
            "-Isrc/usbd",
            "-Isrc/sys/system_stm32h7xx",
            "-Iusbd/usbd_cdc_if",
            "-Iusbd/usbd_desc",
            "-Iusbd/usbd_con",
            "-IDrivers/CMSIS/Include",
            "-IDrivers/STM32H7xx_HAL_Driver/Inc",
            "-IDrivers/CMSIS/Device/Include",
            "-IDrivers/Middlewares/Third_Party/FatFs/inc",
            "-IDrivers/CMSIS/DSP/include"
        ]
    }
  }